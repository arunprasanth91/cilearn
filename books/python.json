    "    sum = 0\n",
    "    for num in range(10000000):\n",
    "        sum +=num\n",
    "    print(f\"sum is {sum}\",end=\"\\n\")\n",
    "    print(\"==========================\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def count_nos():\n",
    "    count = 0\n",
    "    for num in range(10000000):\n",
    "        count+=1\n",
    "    print(f\"Cout is: {count}\",end=\"\\n\")"
   ]
